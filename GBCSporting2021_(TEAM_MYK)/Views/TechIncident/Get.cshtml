
@model Technician
@{ ViewBag.Title = "Technician"; }

<h1>@TempData["message"]</h1>
<form asp-controller="TechIncident" asp-action="List" method="GET">
    <div class="col-md-4">
        <div class="input-group mb-6">
            <label class="input-group-text">@ViewBag.Title</label>
            <select class="form-select" id="id" name="id" asp-items="@(new SelectList(ViewBag.listOfTech, "TechnicianId", "Name"))">
                <option selected disabled>--  Select Product  --</option>
            </select>

        </div>
    </div>
    <br />
    <div class="col-md-7">
        <button class="btn btn-primary" type="submit">@ViewBag.Action</button>
    </div>
</form>